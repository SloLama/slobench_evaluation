{
    "model": {
        "library": "nemo",
        "path": "/path/to/the/model/gams.nemo"
    },
    "prompt_template": "Uporabnik: {instruction}\n\n{input}\nAsistent:",
    "benchmarks": [
        {
            "dataset": "BoolQ",
            "human_translated": true,
            "machine_translated": true,
            "seed": 5,
            "k": [0, 1, 2, 3],
            "prefix": {
                "passage": "Besedilo: ",
                "question": "Vprašanje: ",
                "output": "Odgovor: "
            },
            "evaluation": {
                "majority_correlation": true,
                "last_example_correlation": true,
                "ci": {
                    "type": "std",
                    "alpha": 0.95
                }
            }
        },
        {
            "dataset": "MultiRC",
            "human_translated": true,
            "machine_translated": true,
            "seed": 5,
            "k": [0, 1, 2, 3],
            "prefix": {
                "text": "Besedilo: ",
                "question": "Vprašanje: ",
                "answers": "Seznam možnih odgovorov:\n",
                "output": "Številke pravilnih odgovorov: "
            },
            "evaluation": {
                "majority_correlation": true,
                "last_example_correlation": true,
                "ci": {
                    "correlation": {
                        "type": "std",
                        "alpha": 0.95
                    },
                    "exact_match": {
                        "type": "std",
                        "alpha": 0.95
                    },
                    "per_question_f1": {
                        "type": "std",
                        "alpha": 0.95
                    },
                    "all_answers_f1": {
                        "type": "quantile_bootstrap",
                        "alpha": 0.95,
                        "bootstrap_samples": 1000,
                        "seed": 42
                    }
                }
            }
        },
        {
