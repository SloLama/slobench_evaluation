{
    "model": {
        "library": "huggingface",
        "path": "cjvt/GaMS-1B-Chat"
    },
    "prompt_scheme_file": "prompt_templates_example.json",
    "benchmarks": [
        {
            "dataset": "BoolQ",
            "human_translated": true,
            "machine_translated": true,
            "seed": 5,
            "evaluation": {
                "majority_correlation": true,
                "last_example_correlation": true,
                "ci": {
                    "type": "std",
                    "alpha": 0.95
                }
            }
        },
        {
            "dataset": "MultiRC",
            "human_translated": true,
            "machine_translated": true,
            "seed": 5,
            "evaluation": {
                "majority_correlation": true,
                "last_example_correlation": true,
                "ci": {
                    "correlation": {
                        "type": "std",
                        "alpha": 0.95
                    },
                    "exact_match": {
                        "type": "std",
                        "alpha": 0.95
                    },
                    "per_question_f1": {
                        "type": "std",
                        "alpha": 0.95
                    },
                    "all_answers_f1": {
                        "type": "quantile_bootstrap",
                        "alpha": 0.95,
                        "bootstrap_samples": 1000,
                        "seed": 42
                    }
                }
            }
        },
        {
            "dataset": "WSC",
            "human_translated": true,
            "machine_translated": false,
            "seed": 5,
