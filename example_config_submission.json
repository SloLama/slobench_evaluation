{
    "model": {
        "library": "huggingface",
        "path": "cjvt/GaMS-1B-Chat"
    },
    "prompt_template": "{instruction}\n\n{input}",
    "benchmarks": [
        {
            "dataset": "BoolQ",
            "human_translated": true,
            "machine_translated": false,
            "seed": 5,
            "k": 0,
            "prefix": {
                "passage": "Besedilo: ",
                "question": "Vprašanje: ",
                "output": "Odgovor: "
            }
        },
        {
            "dataset": "CB",
            "human_translated": true,
            "machine_translated": false,
            "seed": 5,
            "k": 3,
            "prefix": {
                "premise": "Besedilo: ",
                "hypothesis": "Hipoteza: ",
                "output": "Odgovor: "
            }
        },
        {
            "dataset": "COPA",
            "human_translated": true,
            "machine_translated": false,
            "seed": 5,
            "k": 4,
            "prefix": {
                "premise": "Trditev: ",
                "choice1": "Hipoteza 1: ",
                "choice2": "Hipoteza 2: ",
                "output": "Številka hipoteze, ki predstavlja {question}: ",
                "question_effect": "posledico dane trditve",
                "question_cause": "vzrok za dano trditev"
            }
        },
        {
            "dataset": "MultiRC",
            "human_translated": true,
            "machine_translated": false,
            "seed": 5,
            "k": 0,
            "prefix": {
                "text": "Besedilo: ",
                "question": "Vprašanje: ",
                "answers": "Seznam možnih odgovorov:\n",
                "output": "Številke pravilnih odgovorov: "
            }
        },
        {
            "dataset": "RTE",
            "human_translated": true,
            "machine_translated": false,
            "seed": 5,
            "k": 5,
            "prefix": {
                "premise": "Besedilo: ",
                "hypothesis": "Hipoteza: ",
                "output": "Odgovor: "
            }
        },
        {
            "dataset": "WSC",
            "human_translated": true,
            "machine_translated": false,
            "seed": 5,
            "k": 1,
            "prefix": {
                "text": "Besedilo: ",
                "question": "Vprašanje: ",
                "output": "Odgovor: "
            }
        },
        {
            "dataset": "NLI",
            "human_translated": true,
            "machine_translated": false,
            "seed": 5,
            "k": 4,
            "prefix": {
                "premise": "Predpostavka: ",
                "hypothesis": "Hipoteza: ",
                "output": "Odgovor: "
            }
        }
    ]
}